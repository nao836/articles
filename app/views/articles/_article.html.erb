<div id="<%= dom_id article %>" class='container article_cassette position-relative'>
  <p>
  <%= link_to article do %>
    <strong>Title:</strong>
    <%= article.title %>
  <% end %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= article.body %>
  </p>


  <%= link_to article.title, article , class: "stretched-link" %>

</div>
